<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE 
    mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Dao.BillDao">
	<insert id="insertBill" parameterType="Pojo.Bill">
		insert into bill(id,Phone,aliaccount,money,billstatus)
		values(#{id},#{Phone},#{aliaccount},#{money},#{billstatus})
	</insert>

</mapper>